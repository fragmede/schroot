"% \\f[CB]echo $SESSION\\fR\\[CR]\n"
"sid-snap-46195b04-0893-49bf-beb8-0d4ccc899f0f\n"
msgstr ""
"% \\f[CB]SESSION=$(schroot -b -c sid-snap)\\fR\\[CR]\n"
